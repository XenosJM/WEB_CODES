	<div id="output"></div>
	
	<script type="text/javascript">
		function input(){
			let output = document.getElementById('output');
			let x = document.getElementById('input').value;
			// input.value : 입력 받은 데이터를 저장
			console.log(x);
			console.log(typeof x);
		}
	</script>
